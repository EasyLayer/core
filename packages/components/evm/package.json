{
  "name": "@easylayer/evm",
  "version": "0.6.8",
  "description": "",
  "type": "commonjs",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "module": "./dist/esm/index.mjs",
      "default": "./dist/index.js"
    },
    "./dist/*": {
      "types": "./dist/*.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clear": "yarn run clear:dist && yarn run clear:node_modules",
    "clear:node_modules": "rimraf node_modules",
    "clear:dist": "rimraf dist && rimraf .rollup.cache",
    "test:unit": "jest --maxWorkers=1 -c ./jest.config.unit.mjs",
    "lint": "eslint -c eslint.config.mjs \"src/**/*.ts\"",
    "lint:fix": "eslint -c eslint.config.mjs \"src/**/*.ts\" --fix",
    "format": "prettier -—Å prettier.config.mjs --write \"src/**/*.ts\"",
    "build:cjs": "tsc -b tsconfig.cjs.json",
    "build:esm": "rollup --config",
    "build": "yarn build:cjs && yarn build:esm",
    "prepublishOnly": "yarn clear:dist && yarn build"
  },
  "author": "Yaroslav Pavliuk",
  "license": "",
  "packageManager": "yarn@4.5.0",
  "dependencies": {
    "@easylayer/common": "0.6.8",
    "@nestjs/common": "10.2.10",
    "@nestjs/core": "10.2.10",
    "@nestjs/swagger": "7.1.17",
    "async-mutex": "0.5.0",
    "ethers": "6.13.5",
    "lodash": "4.17.21",
    "piscina": "4.1.0",
    "reflect-metadata": "0.1.14",
    "rxjs": "7.8.1",
    "uuid": "9.0.1",
    "web3": "4.16.0"
  },
  "devDependencies": {
    "@nestjs/testing": "10.2.10",
    "@rollup/plugin-typescript": "11.1.6",
    "@types/express": "4.17.21",
    "@types/jest": "29.5.11",
    "@types/lodash": "4.17.7",
    "@types/node": "20.10.4",
    "@types/uuid": "9.0.7",
    "jest": "29.7.0",
    "rollup": "4.17.2",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.2",
    "typescript": "5.3.3"
  }
}
